- users.each do |user|
  %tr
    %td= user.id
    %td= user.complete_name
    %td= user.email
    %td= user.role.titleize
    %td
      = link_to user, class: "ca-actions" do
        %i.fa.fa-file-text.text-navy
      = link_to edit_user_path(user), class: "ca-actions" do
        %i.fa.fa-pencil.text-navy
      = link_to user, { method: :delete, data: { confirm: "¿Estás seguro?" } } do
        %i.fa.fa-remove.text-navy
